# Install libraries to MicroPython target device using "rshell".

# Upload lib folder
#rsync ./lib /flash/lib

mkdir /flash/lib-mpy-pycom
rsync -m lib-mpy-pycom /flash/lib-mpy-pycom

cp lib-mpy-pycom/umal.mpy /flash/lib

cp pybytes_config.json /flash

cp boot.py /flash
cp main.py /flash
cp settings.py /flash
